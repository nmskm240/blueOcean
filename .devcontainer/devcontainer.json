{
	"name": "blueOcean",
	"dockerComposeFile": [
		"compose.yaml"
	],
	"forwardPorts": [
		8501, // streamlit
	],
	"features": {
		"ghcr.io/warrenbuckley/codespace-features/sqlite": "1"
	},
	"service": "app",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"openai.chatgpt",
				"ms-toolsai.jupyter",
				"ms-python.isort",
				"ms-python.black-formatter",
				"ms-toolsai.datawrangler",
				"Mebrahtom.plantumlpreviewer",
				"jebbs.plantuml"
			]
		}
	},
	"postCreateCommand": "pip install -r requirements.txt"
}